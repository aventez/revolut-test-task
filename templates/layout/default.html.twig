<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}{{ 'app.title'|trans }}{% endblock %}</title>

    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block head_javascripts %}
        {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>
<body class="c-app">
{% include 'global/_flashes.html.twig' %}
<div class="c-wrapper">
    <div class="c-body">
        <main class="c-main">
            <div class="container-fluid">
                {% block body %}{% endblock %}
            </div>
        </main>
    </div>
</div>
{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>